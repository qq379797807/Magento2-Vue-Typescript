{
    "presets": [
        [
            "@babel/preset-env",
            {
                "modules": false,
                "loose": true
            }
        ]
    ],
    "plugins": [
        "transform-vue-jsx",
        "@babel/plugin-transform-runtime"
    ],
    "comments": false,
    "env": {
        "test": {
            "presets": [
                "@babel/preset-env"
            ],
            "plugins": [
                "istanbul"
            ]
        }
    }
}